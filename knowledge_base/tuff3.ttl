<http://www.semanticweb.org/ljutach/ontologies/2023/8/tuff3> rdf:type owl:Ontology .

#################################################################
#    Annotation properties
#################################################################

###  http://www.w3.org/2000/01/rdf-schema#comment
rdfs:comment rdfs:range rdfs:Literal ;
             rdfs:domain owl:Thing .


###  http://www.w3.org/2000/01/rdf-schema#isDefinedBy
rdfs:isDefinedBy rdfs:range rdfs:Literal ;
                 rdfs:domain owl:Thing .


#################################################################
#    Object Properties
#################################################################

###  http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#hasQuality
<http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#hasQuality> rdf:type owl:ObjectProperty ;
                                                                   rdfs:domain <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Object> ;
                                                                   rdfs:range <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Quality> ;
                                                                   owl:propertyDisjointWith :partecipant .


###  http://www.semanticweb.org/ljutach/ontologies/2023/8/tuff3#actedIn
:actedIn rdf:type owl:ObjectProperty ;
         rdfs:subPropertyOf :partecipant ;
         owl:inverseOf :actor ;
         rdfs:domain :Relation ;
         rdfs:range :Entity .


###  http://www.semanticweb.org/ljutach/ontologies/2023/8/tuff3#actor
:actor rdf:type owl:ObjectProperty ;
       rdfs:subPropertyOf :partecipant ;
       rdfs:domain :Entity ;
       rdfs:range :Relation ;
       owl:propertyDisjointWith :undergoer .


###  http://www.semanticweb.org/ljutach/ontologies/2023/8/tuff3#hasAttr
:hasAttr rdf:type owl:ObjectProperty ;
         rdfs:subPropertyOf <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#hasQuality> ;
         owl:inverseOf :isAttributeOf ;
         rdfs:domain :Entity ;
         rdfs:range :Attribute .


###  http://www.semanticweb.org/ljutach/ontologies/2023/8/tuff3#hasCondition
:hasCondition rdf:type owl:ObjectProperty ;
              rdfs:subPropertyOf :hasAttr ;
              owl:inverseOf :isConditionOf ;
              rdfs:domain :Entity ;
              rdfs:range :Condition .


###  http://www.semanticweb.org/ljutach/ontologies/2023/8/tuff3#hasException
:hasException rdf:type owl:ObjectProperty ;
              rdfs:subPropertyOf :hasAttr ;
              owl:inverseOf :isExceptionOf ;
              rdfs:domain :Entity ;
              rdfs:range :Exception .


###  http://www.semanticweb.org/ljutach/ontologies/2023/8/tuff3#hasFunction
:hasFunction rdf:type owl:ObjectProperty ;
             rdfs:subPropertyOf :hasAttr ;
             owl:inverseOf :isFunctionOf ;
             rdfs:domain :Entity ;
             rdfs:range :Function .


###  http://www.semanticweb.org/ljutach/ontologies/2023/8/tuff3#hasMetaAttr
:hasMetaAttr rdf:type owl:ObjectProperty ;
             rdfs:subPropertyOf <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#hasQuality> ;
             owl:inverseOf :isMetaAttrOf ;
             rdfs:domain :Attribute ;
             rdfs:range :Attribute .


###  http://www.semanticweb.org/ljutach/ontologies/2023/8/tuff3#hasMetric
:hasMetric rdf:type owl:ObjectProperty ;
           rdfs:subPropertyOf :hasAttr ;
           owl:inverseOf :isMetricOf ;
           rdfs:domain :Entity ;
           rdfs:range :Metric .


###  http://www.semanticweb.org/ljutach/ontologies/2023/8/tuff3#hasReference
:hasReference rdf:type owl:ObjectProperty ;
              rdfs:subPropertyOf :hasAttr ;
              owl:inverseOf :isReferenceOf ;
              rdfs:domain :Entity ;
              rdfs:range :Reference .


###  http://www.semanticweb.org/ljutach/ontologies/2023/8/tuff3#isAttributeOf
:isAttributeOf rdf:type owl:ObjectProperty ;
               rdfs:subPropertyOf <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#hasQuality> ;
               rdfs:domain :Attribute ;
               rdfs:range :Entity .


###  http://www.semanticweb.org/ljutach/ontologies/2023/8/tuff3#isConditionOf
:isConditionOf rdf:type owl:ObjectProperty ;
               rdfs:subPropertyOf :hasAttr ;
               rdfs:domain :Condition ;
               rdfs:range :Entity .


###  http://www.semanticweb.org/ljutach/ontologies/2023/8/tuff3#isExceptionOf
:isExceptionOf rdf:type owl:ObjectProperty ;
               rdfs:subPropertyOf :hasAttr ;
               rdfs:domain :Exception ;
               rdfs:range :Entity .


###  http://www.semanticweb.org/ljutach/ontologies/2023/8/tuff3#isFunctionOf
:isFunctionOf rdf:type owl:ObjectProperty ;
              rdfs:subPropertyOf :hasAttr ;
              rdfs:domain :Function ;
              rdfs:range :Entity .


###  http://www.semanticweb.org/ljutach/ontologies/2023/8/tuff3#isMetaAttrOf
:isMetaAttrOf rdf:type owl:ObjectProperty ;
              rdfs:subPropertyOf <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#hasQuality> ;
              rdfs:domain :Attribute ;
              rdfs:range :Attribute .


###  http://www.semanticweb.org/ljutach/ontologies/2023/8/tuff3#isMetricOf
:isMetricOf rdf:type owl:ObjectProperty ;
            rdfs:subPropertyOf :hasAttr ;
            rdfs:domain :Metric ;
            rdfs:range :Entity .


###  http://www.semanticweb.org/ljutach/ontologies/2023/8/tuff3#isReferenceOf
:isReferenceOf rdf:type owl:ObjectProperty ;
               rdfs:subPropertyOf :hasAttr ;
               rdfs:domain :Reference ;
               rdfs:range :Entity .


###  http://www.semanticweb.org/ljutach/ontologies/2023/8/tuff3#partecipant
:partecipant rdf:type owl:ObjectProperty ;
             rdfs:domain :Entity ;
             rdfs:range :Relation .


###  http://www.semanticweb.org/ljutach/ontologies/2023/8/tuff3#undergoer
:undergoer rdf:type owl:ObjectProperty ;
           rdfs:subPropertyOf :partecipant ;
           owl:inverseOf :underwent ;
           rdfs:domain :Entity ;
           rdfs:range :Relation .


###  http://www.semanticweb.org/ljutach/ontologies/2023/8/tuff3#underwent
:underwent rdf:type owl:ObjectProperty ;
           rdfs:subPropertyOf :partecipant ;
           rdfs:domain :Relation ;
           rdfs:range :Entity .


#################################################################
#    Data properties
#################################################################

###  http://www.semanticweb.org/ljutach/ontologies/2023/8/tuff3#hasFullText
:hasFullText rdf:type owl:DatatypeProperty ;
             rdfs:domain :Reference ;
             rdfs:range rdfs:Literal .


#################################################################
#    Classes
#################################################################

###  http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Event
<http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Event> rdf:type owl:Class .


###  http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Object
<http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Object> rdf:type owl:Class .


###  http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Quality
<http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Quality> rdf:type owl:Class .


###  http://www.semanticweb.org/ljutach/ontologies/2023/8/tuff3#Attribute
:Attribute rdf:type owl:Class ;
           rdfs:subClassOf <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Quality> ;
           owl:disjointWith :Entity ,
                            :Relation ;
           rdfs:comment "Encompasses characteristic aspects of an entity that cannot exist without it"^^rdfs:Literal ;
           rdfs:isDefinedBy "Dasiopoulou, Stamatia, Steffen Lohmann, Joan Codina and Leo Wanner. “Representing and Visualizing Text as Ontologies: A Case from the Patent Domain.” VOILA@ISWC (2015)"^^rdfs:Literal .


###  http://www.semanticweb.org/ljutach/ontologies/2023/8/tuff3#Condition
:Condition rdf:type owl:Class ;
           rdfs:subClassOf :Attribute ;
           owl:disjointWith :Exception ,
                            :Function ,
                            :Metric ,
                            :Reference ;
           rdfs:comment "A condition to be fulfilled"^^rdfs:Literal .


###  http://www.semanticweb.org/ljutach/ontologies/2023/8/tuff3#Definition
:Definition rdf:type owl:Class ;
            rdfs:subClassOf :Relation ;
            rdfs:comment "A predicate connecting an Explanandum to an Explanans"^^rdfs:Literal .


###  http://www.semanticweb.org/ljutach/ontologies/2023/8/tuff3#Entity
:Entity rdf:type owl:Class ;
        rdfs:subClassOf <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Object> ;
        rdfs:comment "Subsumes the set of physical objects, processes, and substances"^^rdfs:Literal ;
        rdfs:isDefinedBy "Dasiopoulou, Stamatia, Steffen Lohmann, Joan Codina and Leo Wanner. “Representing and Visualizing Text as Ontologies: A Case from the Patent Domain.” VOILA@ISWC (2015)" .


###  http://www.semanticweb.org/ljutach/ontologies/2023/8/tuff3#Exception
:Exception rdf:type owl:Class ;
           rdfs:subClassOf :Attribute ;
           owl:disjointWith :Function ,
                            :Metric ,
                            :Reference ;
           rdfs:comment "A condition that must be unfulfilled"^^rdfs:Literal .


###  http://www.semanticweb.org/ljutach/ontologies/2023/8/tuff3#Explanandum
:Explanandum rdf:type owl:Class ;
             rdfs:subClassOf :Entity ;
             owl:disjointWith :Explanans ;
             rdfs:comment "The subject of a definition"^^rdfs:Literal .


###  http://www.semanticweb.org/ljutach/ontologies/2023/8/tuff3#Explanans
:Explanans rdf:type owl:Class ;
           rdfs:subClassOf :Entity ;
           rdfs:comment "An expression defining an Explanandum"^^rdfs:Literal .


###  http://www.semanticweb.org/ljutach/ontologies/2023/8/tuff3#Function
:Function rdf:type owl:Class ;
          rdfs:subClassOf :Attribute ;
          owl:disjointWith :Metric ,
                           :Reference ;
          rdfs:comment "An expected role"^^rdfs:Literal .


###  http://www.semanticweb.org/ljutach/ontologies/2023/8/tuff3#Metric
:Metric rdf:type owl:Class ;
        rdfs:subClassOf :Attribute ;
        owl:disjointWith :Reference ;
        rdfs:comment "A quantitative dimension"^^rdfs:Literal .


###  http://www.semanticweb.org/ljutach/ontologies/2023/8/tuff3#Reference
:Reference rdf:type owl:Class ;
           rdfs:subClassOf :Attribute ;
           rdfs:comment "A reference to a paragraph in the main source (TUFF) or in other legislative sources"^^rdfs:Literal .


###  http://www.semanticweb.org/ljutach/ontologies/2023/8/tuff3#Relation
:Relation rdf:type owl:Class ;
          rdfs:subClassOf <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Event> ;
          rdfs:comment "captures n-ary interrelations between entities"^^rdfs:Literal ;
          rdfs:isDefinedBy "Dasiopoulou, Stamatia, Steffen Lohmann, Joan Codina and Leo Wanner. “Representing and Visualizing Text as Ontologies: A Case from the Patent Domain.” VOILA@ISWC (2015)." .


